{
	"reporter": [
		"lcov",
		"text"
	],
	"include": [
		"server/**/*.js"
	],
	"exclude": [
		"node_modules",
		"bin",
		"coverage",
		"test",
		"**/config/**",
		"**/helpers/models.**.js",
		"server/models/index.js",
		"server/index.js",
		"server/express.js",
		"server/utils/contact-generator.js"
	],
	"sourceMap": false,
	"instrument": false,
	"cache": true,
	"all": true,
	"extension": [
		".js"
	]
}
